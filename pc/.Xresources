*background: [93]#1f1f1f
*foreground: #c6c6c6
*cursorColor: #c6c6c6

*color0:  #828282	
*color8:  #828282
*color1:  #cc6666 	
*color9:  #cc6666 	
*color2:  #8abeb7 	
*color10: #8abeb7	
*color3:  #f0c674	
*color11: #f0c674	
*color4:  #8abeb7	
*color12: #8abeb7	
*color5:  #b294bb	
*color13: #b294bb	
*color6:  #8abeb7	
*color14: #8abeb7	
*color7:  #828282	 
*color15: #c6c6c6	

URxvt.font: xft:Droid Sans Mono:size=10.5
URxvt.depth:                      32
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*saveLines:                  5000
URxvt*internalBorder: 5
URxvt*geometry: 70x19

URxvt.perl-ext-common: default, resize-font
URxvt.keysym.C-minus: resize-font:smaller
URxvt.keysym.C-plus: resize-font:bigger
URxvt.keysym.C-equal: resize-font:reset


rofi.color-enabled: true
rofi.color-window: #AA1f1f1f, #8abeb7, #1f5951

rofi.color-normal: #AA1f1f1f, #1f5951, #AA1f1f1f, #AA1f1f1f, #8abeb7
rofi.color-active: #AA1f1f1f, #8abeb7, #AA1f1f1f, #AA1f1f1f, #8abeb7
rofi.color-urgent: #AA1f1f1f, #1f5951, #AA1f1f1f, #AA0000ff, #8abeb7

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: Droid Sans Mono 14
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true

! Normal copy-paste keybindings without perls
URxvt.iso14755:                   false
URxvt.keysym.Shift-Control-V:     eval:paste_clipboard
URxvt.keysym.Shift-Control-C:     eval:selection_to_clipboard
!Xterm escape codes, word by word movement
URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B
